{
  "schemaVersion": 1,
  "id": "librarian",
  "version": "${version}",
  "name": "Librarian",
  "description": "Improves the saved hotbar system by adding new features and increasing the overall capacity with a page system",
  "authors": [
    "videogamesm12"
  ],
  "contact": {
    "homepage": "https://github.com/VideoGameSmash12/Librarian",
    "sources": "https://github.com/VideoGameSmash12/Librarian",
    "issues": "https://github.com/VideoGameSmash12/Librarian/issues"
  },
  "license": "GPL-3.0",
  "icon": {
    "64": "assets/librarian/icon-64.png",
    "128": "assets/librarian/icon.png",
    "256": "assets/librarian/icon-256.png"
  },
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.videogamesm12.librarian.Librarian"
    ],
    "librarian-listener": [
      "me.videogamesm12.librarian.listener.EventLogger"
    ]
  },
  "breaks": {
    "hotbarsplus": "*"
  }
}
